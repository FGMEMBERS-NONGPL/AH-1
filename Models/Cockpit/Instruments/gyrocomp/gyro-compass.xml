<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>gyro-compass.ac</path>
	
 <animation>
  <type>scale</type>
  <x-offset>0.8</x-offset>
  <y-offset>0.8</y-offset>
  <z-offset>0.8</z-offset>
 </animation>

  <animation>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <object-name>gyrobug</object-name>
  </animation>


<animation>
	<type>rotate</type>
	<object-name>face</object-name>
	    <property>orientation/heading-deg</property>
		<factor>1</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>


</PropertyList>
