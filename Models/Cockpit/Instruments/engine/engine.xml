<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>engine.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.25</x-offset>
  <y-offset>0.25</y-offset>
  <z-offset>0.25</z-offset>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
      <entry><ind>   0 </ind><dep> -260 </dep></entry>
      <entry><ind> 220 </ind><dep>  8 </dep></entry>
      <entry><ind> 400 </ind><dep>   70 </dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
