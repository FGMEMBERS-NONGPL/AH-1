<!-- Collective -->

<animation>
<type>rotate</type>
<object-name>collective</object-name>
<property>/controls/engines/engine/throttle</property>
<factor>-22</factor>
<center>
<x-m>-0.34</x-m>
<y-m>1.5</y-m>
<z-m>-0.65</z-m>
</center>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<!-- Pedals -->

<animation>
<type>translate</type>
<object-name>l_pedal</object-name>
<property>controls/flight/rudder</property>
<factor>0.07</factor>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>translate</type>
<object-name>r_pedal</object-name>
<property>controls/flight/rudder</property>
<factor>0.07</factor>
<axis>
<x>0</x>
<y>-1</y>
<z>0</z>
</axis>
</animation>

<!-- Stick -->

<animation>
<type>rotate</type>
<object-name>yoke</object-name>
<object-name>stick-rubber-hose-0</object-name>
<property>controls/flight/elevator</property>
<factor>13</factor>
<center>
<x-m>0</x-m>
<y-m>1.8</y-m>
<z-m>-0.95</z-m>
</center>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>yoke</object-name>
<object-name>stick-rubber-hose-0</object-name>
<property>controls/flight/aileron</property>
