<center>
<x-m>0</x-m>
<y-m>1.8</y-m>
<z-m>-0.95</z-m>
</center>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>translate</type>
<object-name>stick-rubber-hose-1</object-name>
