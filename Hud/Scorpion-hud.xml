<label>
	<name>Ultracapacitor</name>
	<width>1</width>
	<height>1</height>
	<x>0</x>
	<y>-85</y>
	<halign>center</halign>
	<prefix>CAP </prefix>	
	<format>%.0f</format>
	<postfix>pct</postfix>
	<box>false</box>
	<option>center</option>
	<input>
		<property>fdm/jsbsim/propulsion/tank[5]/pct-full</property>
	</input>
   	<condition>
		<and>
		<equals>
			<property>fdm/jsbsim/T4T/AGP/mode-1</property>
			<value>1</value>
		</equals>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[2]/propeller-rpm</property>
				<value>1000</value>
			</greater-than>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[3]/propeller-rpm</property>
				<value>1000</value>
			</greater-than>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[4]/propeller-rpm</property>
				<value>1000</value>
			</greater-than>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[5]/propeller-rpm</property>
				<value>1000</value>
			</greater-than>				
			<greater-than>
				<property>/position/altitude-agl-ft</property>
				<value>20</value>
			</greater-than>				
		</and>
	</condition>
	<blinking>
		<interval>0.5</interval>
		<condition>
			<less-than>
				<property>fdm/jsbsim/propulsion/tank[5]/pct-full</property>
				<value>20</value>
			</less-than>
		</condition>
	</blinking>
	<blinking>
		<interval>0.25</interval>
		<condition>
			<less-than>
				<property>fdm/jsbsim/propulsion/tank[5]/pct-full</property>
				<value>5</value>
			</less-than>
		</condition>
	</blinking>	
</label>



<label>
	<width>1</width>
	<height>1</height>
	<x>0</x>
	<y>-85</y>
	<box>false</box>
	<format>ENGINES RUNNING</format>
	<halign>center</halign>
   	<condition>
		<and>
			<less-than-equals>
				<property>/position/altitude-agl-ft</property>
				<value>20</value>
			</less-than-equals>		
			<equals>
				<property>fdm/jsbsim/T4T/AGP/mode-1</property>
				<value>1</value>
			</equals>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[0]/set-running</property>
				<value>0</value>
			</greater-than>
			<greater-than>
				<property>fdm/jsbsim/propulsion/engine[1]/set-running</property>
				<value>0</value>
			</greater-than>
			<less-than>
				<property>fdm/jsbsim/propulsion/engine[2]/propeller-rpm</property>
				<value>1700</value>
			</less-than>
			<less-than>
				<property>fdm/jsbsim/propulsion/engine[3]/propeller-rpm</property>
				<value>1700</value>
			</less-than>
			<less-than>
				<property>fdm/jsbsim/propulsion/engine[4]/propeller-rpm</property>
				<value>1700</value>
			</less-than>
			<less-than>
				<property>fdm/jsbsim/propulsion/engine[5]/propeller-rpm</property>
				<value>1700</value>
			</less-than>			
		</and>			
	</condition>
</label>

