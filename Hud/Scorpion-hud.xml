<PropertyList>
<name>Avatar Scorpion HUD</name>

<enable3d>true</enable3d>

	<!-- EMERGENCY MODE -->

<label>
	<name>F MODE 0</name>
	<width>1</width>
	<height>5</height>
	<x>0</x>
	<y>-107.5</y>
	<halign>center</halign>
	<format>EMERGENCY MODE</format>
	<box>false</box>
	<option>center</option>
  	<condition>
		<equals>
			<property>fdm/jsbsim/T4T/AGP/mode-0</property>
			<value>1</value>
		</equals>
	</condition>
	<blinking>
